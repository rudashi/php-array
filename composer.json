{
	"name": "rudashi/php-array",
	"description": "Fluent wrapper for working with arrays of data, allow to chain methods similar to jQuery or Laravel Collections",
	"keywords": [
        "php", 
        "array", 
        "map", 
        "collection"
    ],
	"require": {
		"php": "^7.3|^8.0"
	},
	"require-dev": {
        "phpunit/phpunit": "^9.3.3"
	},
	"autoload": {
		"psr-4": {
			"Rudashi\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ],
		"classmap": [
			"src/"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}